{
	"name": "@i/repository",
	"exports": {
		"./IndieRepository": "./src/IndieRepository.ts",
		"./pool": "./src/pool.ts"
	},
	"scripts": {
		"typecheck": "tsc --noEmit",

		"db": "docker-compose --project-name indiedb up -d",
		"db:gen": "zapatos",
		"db:up": "dbmate up",
		"db:down": "dbmate down",
		"db:towns": "cd db/data && sh ./install.sh",
		"db:mock": "tsx db/data/mock.ts",
		"db:remock": "tsx db/data/delete.ts && pnpm run db:mock"
	},
	"dependencies": {
		"pg": "^8.13.0",
		"zapatos": "^6.4.4"
	},
	"devDependencies": {
		"@i/tsconfig": "workspace:*"
	},
	"version": "1.0.0",
	"private": true,
	"license": "UNLICENSED",
	"type": "module"
}
